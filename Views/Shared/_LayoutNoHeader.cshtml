<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <!-- Vendor CSS Template -->
    @Styles.Render("~/Content/assets/vendor/bootstrap/css/bootstrap.min.css")
    @Styles.Render("~/Content/assets/vendor/bootstrap-icons/bootstrap-icons.css")
    @Styles.Render("~/Content/assets/vendor/remixicon/remixicon.css")
    @Styles.Render("~/Content/assets/vendor/boxicons/css/boxicons.min.css")
    @Styles.Render("~/Content/assets/vendor/quill/quill.snow.css")
    @Styles.Render("~/Content/assets/vendor/quill/quill.bubble.css")
    @Styles.Render("~/Content/assets/vendor/simple-datatables/style.css")
    @Styles.Render("~/Content/assets/css/style.css")

    <!-- Bundles -->
    @Scripts.Render("~/bundles/modernizr")



    <!-- Google Fonts -->
    <link href="https://fonts.gstatic.com" rel="preconnect">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">




</head>
<body>
    <main>
        @RenderBody()
    </main><!-- End #main -->
    <!-- ======= Footer ======= -->
    <footer id="footer" class="footer">
        <div class="copyright">
            &copy; Copyright <strong><span>NiceAdmin</span></strong>. All Rights Reserved
        </div>
        <div class="credits">
            <!-- All the links in the footer should remain intact. -->
            <!-- You can delete the links only if you purchased the pro version. -->
            <!-- Licensing information: https://bootstrapmade.com/license/ -->
            <!-- Purchase the pro version with working PHP/AJAX contact form: https://bootstrapmade.com/nice-admin-bootstrap-admin-html-template/ -->
            Designed by <a href="https://bootstrapmade.com/">BootstrapMade</a>
        </div>
    </footer><!-- End Footer -->

    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>




    @*@Scripts.Render("~/bundles/jquery")*@
    @Scripts.Render("~/bundles/bootstrap")

    <!-- Vendor JS Template -->
    @Scripts.Render("~/Content/assets/vendor/bootstrap/js/bootstrap.bundle.js")
    @Scripts.Render("~/Content/assets/vendor/php-email-form/validate.js")
    @Scripts.Render("~/Content/assets/vendor/quill/quill.min.js")
    @Scripts.Render("~/Content/assets/vendor/tinymce/tinymce.min.js")
    @Scripts.Render("~/Content/assets/vendor/simple-datatables/simple-datatables.js")
    @Scripts.Render("~/Content/assets/vendor/chart.js/chart.min.js")
    @Scripts.Render("~/Content/assets/vendor/apexcharts/apexcharts.min.js")
    @Scripts.Render("~/Content/assets/vendor/echarts/echarts.min.js")
    @Scripts.Render("~/Content/assets/js/main.js")





    @RenderSection("scripts", required: false)
</body>
</html>
